
<!-- Content -->
<div class="content clearfix">

  <!-- Back Top -->
  <div class="scroll-to-top">
    <a href="#">Back Up Top</a>
  </div>
  <!-- Back Top End -->

  <!-- Fullscreen Slider Section -->
  <section class="section-block featured-media window-height tm-slider-parallax-container">

  <div class="tm-slider-container full-width-slider" data-parallax data-external-padding="60" data-animation="slide" data-scale-under="960">
    <ul class="tms-slides">
      <li class="tms-slide" data-image data-force-fit>
        <div class="tms-content">
          <div class="tms-content-inner center">
            <div class="row">
              <div class="column width-12">
                <h1 class="tms-caption title-xlarge color-charcoal lspacing-medium"
                  data-animate-in="preset:scaleOut;duration:1000ms;">
                  RAD.CAT
                </h1>
                <div class="division"></div>
                <div class="clear"></div>
                <h3 class="tms-caption font-alt-1 color-charcoal lspacing-xlarge"
                  data-animate-in="preset:slideInUpShort;duration:1000ms;delay:200ms;">
                  A Tech Collective
                </h3>
              </div>
            </div>
          </div>
        </div>
        <img data-src="<%=asset_path('intro.png')%>" >
      </li>
    </ul>
  </div>
</section>
  <!-- Fullscreen Slider Section End -->

  <!-- About Intro -->
  <div id="about" class="section-block">
    <div class="row">
      <div class="column width-8 offset-2 center horizon" data-animate-in="preset:slideInUpShort;duration:1000ms;" data-threshold="0.5">
        <p class="text-xlarge weight-bold color-charcoal lead-font">We use community-led research, collaborative design of technology and media, and secure digital strategies for grassroots social movements.</p>
        <a href="/about" class="button medium bkg-black bkg-hover-pink color-white color-hover-white mb-30 text-line-vertical fade-location">Read More</a>
      </div>
    </div>
  </div>
  <!-- About Intro End -->

  <!-- Portfolio Content -->
