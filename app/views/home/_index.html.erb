                    <a class="overlay-link" href="portfolio/mobile-app.html">
                      <img src="images/portfolio/grid/large-margins/project-5-landscape-alt-2.jpg" alt=""/>
                      <span class="overlay-info">
                        <span>
                          <span>
                            <span class="project-title">Macaw</span>
                          </span>
                        </span>
                      </span>
                    </a>
                  </div>
                </div>
